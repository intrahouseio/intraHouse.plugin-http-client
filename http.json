        { "id": "delete", "name": "DELETE" },
        { "id": "patch", "name": "PATCH" },
        { "id": "head", "name": "HEAD" },
        { "id": "connect", "name": "CONNECT" },
        { "id": "options", "name": "OPTIONS" },
        { "id": "trace", "name": "TRACE" }
